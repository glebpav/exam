#include <iostream>
#include <string>
#include <vector>
#include <string>
#include <queue>
#include <stack>
#include <cmath>

using namespace std;

int main(){
    int a, b, count = 0, max = 0, buf;
    cin >> a >> b;

    int num1, num2;
    for (int i = a;i<=b; i++){
        //cout << i << endl;
        buf = abs(i);
        if (buf < 10)continue;
        num1 = buf % 100;
        while (buf >= 100){
            buf/=10;
        }
        //cout << buf << endl;
        num2 = buf;
        buf = abs(i);
        //cout << "yer" <<endl;

        if (num2 == num1)continue;
        if (buf % int(abs(num2-num1)) == 0){
            count++;
            max = i;
            //cout << (num2-num1) <<endl;
        }
    }
    //if (!count)cout << 0 << " " << 0;
    cout << count << " " << max;
}
