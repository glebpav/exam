#include <iostream>
#include <string>
#include <vector>
#include <string>
#include <queue>
#include <stack>
#include <cmath>
#include <fstream>

using namespace std;

int main(){
    int n, to, mA, mB, count;
    cin >> n >> to >> count;
    vector < vector <int> > graf (n, vector <int> (n, 0));
    vector < vector <int> > ways (count, vector <int> (4));
    vector < int > distance(n);
    vector < bool > visited(n);
    int inf = 999999;

    for (int i=0;i<count;i++){
        int x, y,  time, wigth;
        cin >> x >> y >> time >> wigth;
        //cout << wigth << "---" << endl;
        vector <int> buf;
        buf.push_back(x-1);
        buf.push_back(y-1);
        buf.push_back(time);
        buf.push_back(wigth);
        ways[i]=buf;
    }

    cin >> mA >> mB;
    //cout << ways.size() << endl;
    for (int i=0;i<count;i++){
        //cout << ways[i][3] << endl;
        if (mA+mB <= ways[i][3]){
            graf[ways[i][0]][ways[i][1]] = ways[i][2];
        }
    }

    int index, i, u;
    for (i=0; i<n; i++){
        distance[i]=inf;
        visited[i]=false;
    }
    distance[0]=0;
    for (count=0; count<n-1; count++) {
        int min=inf;
        for (i=0; i<n; i++){
            if (!visited[i] && distance[i]<=min) {
                min=distance[i];
                index=i;
            }
        }
        u=index;
        visited[u]=true;
        for (i=0; i<n; i++)
            if (!visited[i] && graf[u][i] && distance[u]!=inf
                    && distance[u]+graf[u][i]<distance[i]){
                distance[i]=distance[u]+graf[u][i];
            }
    }
    if (distance[to-1]!=inf)
        cout << distance[to-1];
    else {
        cout<<-1<<endl;
    }
}
